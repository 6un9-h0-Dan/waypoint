<div class="output">
    {{#if this.lines}}
    {{#each this.lines as |line|}}
    <code>{{line}}</code>
    {{/each}}
    {{else}}
    <EmptyState>
        <p>There are no logs to display for this app yet</p>
        <p>Logs will be displayed when you run actions in the CLI such as <code class="inline">build</code>, <code
                class="inline">deploy</code> or <code class="inline">release</code></p>
    </EmptyState>
    {{/if}}
</div>